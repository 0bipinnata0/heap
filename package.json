{
  "name": "@0bipinnata0/heap",
  "version": "1.0.3",
  "description": "A TypeScript implementation of a min heap data structure",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "heap",
    "min-heap",
    "data-structure",
    "typescript"
  ],
  "author": "0bipinnata0",
  "license": "MIT",
  "homepage": "https://github.com/0bipinnata0/heap",
  "repository": {
    "type": "git",
    "url": "https://github.com/0bipinnata0/heap.git"
  },
  "bugs": {
    "url": "https://github.com/0bipinnata0/heap/issues"
  },
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@vitest/coverage-v8": "^3.0.7",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  }
}